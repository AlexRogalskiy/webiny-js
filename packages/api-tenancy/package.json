    "@webiny/pubsub": "^5.18.3",
    "dataloader": "^2.0.0",
    "mdbid": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@webiny/cli": "^5.18.3",
    "@webiny/project-utils": "^5.18.3",
    "jest": "^26.6.3",
    "jest-dynalite": "^3.2.0",
    "rimraf": "^3.0.2",
