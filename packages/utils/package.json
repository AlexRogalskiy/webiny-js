  "exports": {
    ".": "./dist/index.js",
    "./": [
      "./dist/",
      "./src/"
    ]
  },
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/webiny/webiny-js.git",
